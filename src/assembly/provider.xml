<assembly xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
          xmlns="http://maven.apache.org/ASSEMBLY/2.2.0"
          xsi:schemaLocation="http://maven.apache.org/ASSEMBLY/2.2.0 https://maven.apache.org/xsd/assembly-2.2.0.xsd">
    <id>provider</id>
    <formats>
        <format>jar</format>
    </formats>
    <includeBaseDirectory>false</includeBaseDirectory>
    <fileSets>
        <fileSet>
            <directory>${project.build.outputDirectory}</directory>
            <includes>
                <include>ricciliao/x/cache/CacheKey.class</include>
                <include>ricciliao/x/cache/XCacheConstants.class</include>
                <include>ricciliao/x/cache/ConsumerData.class</include>
                <include>ricciliao/x/cache/ConsumerId.class</include>
                <include>ricciliao/x/cache/CacheQuery.class</include>
                <include>ricciliao/x/cache/CacheQuery$Property.class</include>
                <include>ricciliao/x/cache/CacheQuery$Sort.class</include>
                <include>ricciliao/x/cache/CacheQuery$Support.class</include>
                <include>ricciliao/x/cache/CacheQuery$Sort$Direction.class</include>
                <include>ricciliao/x/cache/ProviderCacheProperties.class</include>
                <include>ricciliao/x/cache/ProviderCacheProperties$StoreProperties.class</include>
                <include>ricciliao/x/cache/ProviderCacheProperties$AdditionalProperties.class</include>
                <include>ricciliao/x/cache/ProviderCacheProperties$ConsumerProperties.class</include>

                <include>ricciliao/x/cache/pojo/*.class</include>
                <include>ricciliao/x/cache/pojo/message/*.class</include>
            </includes>
            <outputDirectory>/</outputDirectory>
        </fileSet>
    </fileSets>
</assembly>